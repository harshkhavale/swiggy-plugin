{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/swiggy-plugin",
  "version": "0.1.0",
  "title": "Swiggy Plugin",
  "category": "widgets",
  "icon": "carrot",
  "description": "An interactive block with the Interactivity API",
  "example": {},
  "attributes": {
    "meals": {
      "type": "array",
      "default": []
    },
    "filter": {
      "type": "string",
      "default": "Indian"
    },
    "search": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "interactivity": true
  },
  "textdomain": "swiggy-plugin",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScriptModule": "file:./view.js"
}